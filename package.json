{
  "name": "node-api-template",
  "version": "1.0.0",
  "description": "",
  "main": "app/server.js",
  "engines": {
    "node": "7.2.0"
  },
  "scripts": {
    "build": "rm -rf .build && cp -rf app .build && babel app -d .build",
    "test": "istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec --harmony --colors app/test/*.spec.js",
    "start": "npm run build && node --harmony-async-await .build/server.js",
    "dev": "DEBUG=koa* nodemon --harmony --debug app/server.js --exec babel-node"
  },
  "author": "Fredrik Lack <fredrik@brynn.se>",
  "license": "ISC",
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "it",
      "before",
      "after"
    ]
  },
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-eslint": "6.0.5",
    "babel-preset-node7": "1.4.0",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "nodemon": "1.9.2",
    "standard": "7.1.2"
  },
  "dependencies": {
    "koa": "2.0.0",
    "koa-bodyparser": "3.1.0",
    "koa-logger": "2.0.0",
    "koa-simple-router": "0.2.0",
    "koa-views": "5.2.0"
  }
}
